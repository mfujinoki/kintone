{
  "manifest_version": 1,
  "version": 1,
  "type": "APP",
  "desktop": {
    "js": [
      "https://js.kintone.com/sweetalert/v1.1.3/sweetalert.min.js",
      "js/desktop.js"
    ],
    "css": [
      "https://js.kintone.com/sweetalert/v1.1.3/sweetalert.css"
    ]
  },
  "icon": "image/icon.png",
  "config": {
    "html": "html/config.html",
    "js": [
      "https://js.kintone.com/jquery/3.3.1/jquery.min.js",
      "js/config.js",
      "js/kintone-ui-component.min.js"
    ],
    "css": [
      "css/51-modern-default.css",
      "css/config.css",
      "css/kintone-ui-component.min.css"
    ],
    "required_params": [
      "status_names",
      "title_text",
      "body_text",
      "button_text"
    ]
  },
  "name": {
    "en": "Workflow confirmation Plug-in"
